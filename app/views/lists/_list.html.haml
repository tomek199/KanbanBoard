.col-md-2{id: "list_" + list.id.to_s}
  .panel.panel-primary
    .panel-heading
      %h2.panel-title
        = best_in_place list, :name, type: :input
        .btn-group.pull-right
          %span.glyphicon.glyphicon-circle-arrow-down.dropdown-toggle{"data-toggle" => "dropdown"}
          %ul.dropdown-menu{role: "menu"}
            %li
              = link_to "Delete", list_list_delete_path(list), remote: true
          
    .panel-body
      